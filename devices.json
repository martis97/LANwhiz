{
    "R1": {
        "superuser": {
            "username": "admin",
            "password": "r1admin",
            "privilege": 15
        },
        "interfaces": {
            "GigabitEthernet1/0": {
                "ipv4": "192.168.5.1/24",
                "ipv6": "2008:ACAB:B323:AADF::1:1/64",
                "description": "Connection to LAN",
                "acl" : {
                    "outbound": [
                        "permit-dns-for-internal"
                    ],
                    "inbound": []
                },
                "nat": "inside"
            },
            "GigabitEthernet2/0": {
                "ipv4": "192.168.1.1/24",
                "ipv6": "",
                "description": "",
                "acl": {
                    "outbound": [
                        "permit-dns-for-internal"
                    ],
                    "inbound": []
                },
                "nat": "inside"
            }
        },
        "lines": {
            "console": {
                "acl": {
                    "outbound": [],
                    "inbound": ["permit-mgmt-remote-access"]
                }
            }
        },
        "routing": {
            "static": {
                "destination_network": "0.0.0.0",
                "destination_subnetmask": "0.0.0.0",
                "forward_interface": "Serial0/0/0"
            },
            "ospf": {
                "instance_id" : 10,
                "redistribute_static" : true,
                "interfaces": [
                    "GigabitEthernet0/0",
                    "GigabitEthernet0/1"
                ],
                "passive_interfaces": [
                    "GigabitEthernet0/2"
                ]
            }
        },
        "acl": {
            "standard": {
                "nat": {
                    "action" : "permit",
                    "source_network": "192.168.0.0/16",
                    "overload": "True"
                }
            },
            "extended": {
                    "permit-established-tcp": {
                        "action" : "permit",
                        "protocol": "tcp",
                        "source_network": "any",
                        "destination_network": "any",
                        "port": "established"
                    },
                    "permit-dns-for-internal": {
                        "action": "permit",
                        "protocol": "udp",
                        "source": "192.168.5.0/24",
                        "destination": "host 192.168.1.3",
                        "port": "eq 53"
                    },
                    "permit-remaining-protocols": {
                        "action": "permit",
                        "protocol": "ip",
                        "source": "any",
                        "destination": "any",
                        "port": ""
                    }
                }
            }
        }   
    }
