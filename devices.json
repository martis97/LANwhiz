{
    "R1": {
        "default_commands": [
            "configure terminal",
            "ipv6 unicast-routing",
            "no ip domain-lookup",
            "service password-encryption"
        ],
        "interfaces": {
            "GigabitEthernet0/0": {
                "ipv4": "192.168.5.1/24",
                "ipv6": "2008:ACAB:B323:AADF::1:1/64",
                "description": "Connection to LAN",
                "acl": {
                    "outbound": [
                        "permit-dns-for-internal"
                    ],
                    "inbound": []
                },
                "nat": "inside"
            },
            "GigabitEthernet2/0": {
                "ipv4": "192.168.1.1/24",
                "ipv6": "",
                "description": "",
                "acl": {
                    "outbound": [
                        "permit-dns-for-internal"
                    ],
                    "inbound": []
                },
                "nat": "inside"
            }
        },
        "lines": {
            "console": {
                "password": "netautoconfig",
                "acl": {
                    "inbound": [],
                    "outbound": []
                },
                "synchronous_logging": true
            },
            "vty": {
                "password": "netautoconfig",
                "acl": {
                    "inbound": [
                        "permit-mgmt-remote-access"
                    ],
                    "outbound": []
                },
                "synchronous_logging": false
            }
        },
        "routing": {
            "static": [
                {
                    "network": "0.0.0.0",
                    "subnetmask": "0.0.0.0",
                    "forward_to": "GigabitEthernet0/0"
                }
            ],
            "ospf": {
                "instance_id": 10,
                "router_id": "1.1.1.1",
                "advertise_static": true,
                "advertise_networks": [
                    "192.168.5.1/24/area 0",
                    "192.168.1.1/24/area 0"
                ],
                "passive_interfaces": [
                    "GigabitEthernet0/0"
                ]
            }
        },
        "acl": {
            "standard": {
                "nat": {
                    "action": "permit",
                    "source": "192.168.0.0/16"
                }
            },
            "extended": {
                "permit-established-tcp": {
                    "action": "permit",
                    "protocol": "tcp",
                    "source": "any",
                    "destination": "any",
                    "port": "established"
                },
                "permit-dns-for-internal": {
                    "action": "permit",
                    "protocol": "udp",
                    "source": "192.168.5.0/24",
                    "destination": "192.168.1.3",
                    "port": "eq 53"
                },
                "permit-remaining-protocols": {
                    "action": "permit",
                    "protocol": "ip",
                    "source": "any",
                    "destination": "any",
                    "port": ""
                }
            }
        },
        "dhcp": [{
            "pool_name": "LANhiz_pool",
            "network": "192.168.5.0/24",
            "default_gateway": "192.168.5.1",
            "dns": "192.168.1.3"
        }]
    },
