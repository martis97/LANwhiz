{
    "hostname": "R2",
    "mgmt_ip": "127.0.0.1",
    "mgmt_port": 5001,
    "username": "admin",
    "password": "netautoconfig",
    "config": {
        "default_commands": [
            "configure terminal",
            "ipv6 unicast-routing",
            "no ip domain-lookup",
            "service password-encryption"
        ],
        "interfaces": {
            "FastEthernet0/0": {
                "ipv4": "192.168.5.1/24",
                "ipv6": "2008:ACAB:B323:AADF::1:1/64",
                "description": "Connection to LAN",
                "acl": {
                    "outbound": [
                        "1"
                    ],
                    "inbound": []
                },
                "nat": "outside"
            }
        },
        "lines": {
            "console": {
                "password": "netautoconfig",
                "acl": {
                    "inbound": [],
                    "outbound": []
                },
                "synchronous_logging": true
            },
            "vty": {
                "password": "netautoconfig",
                "acl": {
                    "inbound": [
                        "105"
                    ],
                    "outbound": []
                },
                "synchronous_logging": false
            }
        },
        "routing": {
            "static": [
                {
                    "network": "0.0.0.0",
                    "subnetmask": "0.0.0.0",
                    "forward_to": "FastEthernet0/0"
                }
            ],
            "ospf": {
                "instance_id": 10,
                "router_id": "1.1.1.1",
                "advertise_static": true,
                "advertise_networks": [
                    "192.168.5.1/24/area 0",
                    "192.168.1.1/24/area 0"
                ],
                "passive_interfaces": [
                    "FastEthernet0/0"
                ]
            }
        },
        "acl": {
            "standard": {
                "1": {
                    "action": "permit",
                    "source": "192.168.0.0/16"
                }
            },
            "extended": {
                "105": {
                    "action": "permit",
                    "protocol": "tcp",
                    "source": "any",
                    "destination": "any",
                    "port": "established"
                },
                "permit-dns-for-internal12": {
                    "action": "permit",
                    "protocol": "udp",
                    "source": "192.168.5.0/24",
                    "destination": "192.168.1.3",
                    "port": "eq 53"
                },
                "permit-remaining-protocols": {
                    "action": "permit",
                    "protocol": "ip",
                    "source": "any",
                    "destination": "any",
                    "port": ""
                }
            }
        },
        "S1": {
            "default_commands": [
                "configure terminal",
                "no ip domain-lookup",
                "service password-encryption"
            ],
            "interfaces": {
                "FastEthernet0/0": {
                    "ipv4": "192.168.5.1/24",
                    "ipv6": "2008:ACAB:B323:AADF::1:1/64",
                    "description": "Connection to LAN",
                    "acl": {
                        "outbound": [
                            "1"
                        ],
                        "inbound": []
                    },
                    "nat": "outside"
                }
            },
            "lines": {
                "console": {
                    "password": "netautoconfig",
                    "acl": {
                        "inbound": [],
                        "outbound": []
                    },
                    "synchronous_logging": true
                },
                "vty": {
                    "password": "netautoconfig",
                    "acl": {
                        "inbound": [
                            "105"
                        ],
                        "outbound": []
                    },
                    "synchronous_logging": false
                }
            }
        }
    }
}