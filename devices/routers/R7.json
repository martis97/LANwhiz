{
    "hostname": "R4",
    "mgmt_ip": "192.168.74.128",
    "mgmt_port": 5003,
    "username": "admin",
    "password": "netautoconfig",
    "config": {
        "default_commands": [
            "ipv6 unicast-routing",
            "no ip domain-lookup",
            "service password-encryption"
        ],
        "interfaces": {
            "GigabitEthernet0/0": {
                "ipv4": "10.1.4.2/24",
                "ipv6": "A:B:C:4::2/64",
                "description": "Connection to R3"
            },
            "GigabitEthernet1/0": {
                "ipv4": "10.1.3.2/24",
                "ipv6": "A:B:C:3::1/64",
                "description": "Connection to R2"
            },
            "Loopback0": {
                "ipv4": "192.168.4.1/24",
                "description": "Supposed LAN"
            }
        },
        "lines": {
            "console": {
                "password": "netautoconfig",
                "synchronous_logging": true
            },
            "vty": {
                "password": "netautoconfig",
                "synchronous_logging": false
            }
        },
        "routing": {
            "ospf": {
                "instance_id": 10,
                "router_id": "4.4.4.4",
                "advertise_static": false,
                "advertise_networks": [
                    "10.1.3.0/24/area 0",
                    "10.1.4.0/24/area 0",
                    "192.168.4.0/24/area 0"
                ],
                "passive_interfaces": [
                    "Loopback0"
                ]
            }
        },
        "acl": {
            "standard": {
                "nat": {
                    "action": "permit",
                    "source": "192.168.0.0/16"
                }
            },
            "extended": {
                "permit-established-tcp": {
                    "action": "permit",
                    "protocol": "tcp",
                    "source": "any",
                    "destination": "any",
                    "port": "established"
                },
                "permit-dns-for-internal": {
                    "action": "permit",
                    "protocol": "udp",
                    "source": "192.168.5.0/24",
                    "destination": "192.168.1.3",
                    "port": "eq 53"
                },
                "permit-remaining-protocols": {
                    "action": "permit",
                    "protocol": "ip", 
                    "source": "any",
                    "destination": "any",
                    "port": ""
                }
            }
        },
        "dhcp": [
            {
                "pool_name": "LANhiz_pool",
                "network": "192.168.4.0/24",
                "default_gateway": "192.168.4.1",
                "dns": "192.168.1.3"
            }
        ]
    }
}
